#!/bin/sh

DIRS="usb-hdd iso-cdrom net squashfs"

lb clean noauto --all ${@}

rm -f *.log

# Remove generated files
rm -f config/binary config/bootstrap config/chroot config/common config/source
rm -fr cache

# Remove empty directories in config tree
find -depth -type d -empty -exec rmdir {} \;

# remove compiled wiki
rm -rf config/chroot_local-includes/usr/share/doc/flujos-vivos/html/  wiki/src/.ikiwiki wiki/src
# remove fuente de wiki de systema vivo
rm -rf config/chroot_local-includes/etc/skel/src
